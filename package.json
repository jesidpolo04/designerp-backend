{
  "name": "notes-automator",
  "version": "1.0.0",
  "description": "Notes automator for scripting older academic notes systems",
  "keywords": [
    "scripting",
    "playwright",
    "scrapper"
  ],
  "license": "ISC",
  "author": "Jesid Polo",
  "type": "module",
  "main": "src/main.ts",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "dev": "tsx watch src/main.ts | pino-pretty",
    "dev:otel": "tsx watch --import ./src/instrumentation.ts src/main.ts"
  },
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.67.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "class-validator-jsonschema": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-validator": "^7.3.0",
    "luxon": "^3.7.2",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.20.6",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.27",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.2.0",
    "pg-mem": "^3.0.5",
    "pino-pretty": "^13.1.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5"
  }
}
